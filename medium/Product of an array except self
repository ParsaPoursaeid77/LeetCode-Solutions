class Solution:
    def productExceptSelf(self, nums: List[int]) -> List[int]:
        n = len(nums) # length of nums 

        prefix = [1] * n
        suffix = [1] * n 
        answer = [1] * n 


        # numbers left of i -> builds product of all elements before i, 

        for i in range(1, n):
            prefix[i] = prefix[i-1] * nums[i-1]

        # numbers right of i -> builds product of all elements after i 
        for i in range(n - 2, -1, -1): # starts at second last 
            suffix[i] = suffix[i+1] * nums[i+1]

        for i in range(n):
            answer[i] = prefix[i] * suffix[i]
        
        return answer 
