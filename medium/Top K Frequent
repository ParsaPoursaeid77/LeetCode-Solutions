class Solution:
    def topKFrequent(self, nums: List[int], k: int) -> List[int]:
        count = {}
        freq = [[] for i in range(len(nums) + 1)]

        for num in nums:
            count[num] = 1 + count.get(num, 0) # loops through nums and counts frequency: if num not in count, start at 0 otherwise add 1 
        for num, cnt in count.items(): # for each number and it's frequency cnt, put it into a freq[cnt] bucket
            freq[cnt].append(num)

        res = [] # start from the back of the freq(highest frequency). add numbers into the res until you have k then return res 
        for i in range(len(freq) - 1, 0, -1):
            for num in freq[i]:
                res.append(num)
                if len(res) == k:
                    return res
